.back_white_box
	- if @user.role == 'contractor'
		- if @user != current_user
			= javascript_include_tag 'Calendar/Controllers/contractor-busyness-controller-show'
		- else
			= javascript_include_tag 'Calendar/Controllers/contractor-busyness-controller'
			= javascript_include_tag 'Calendar/Controllers/show-all-events-controller'
		#contractor_skills
			.row-fluid
				.span5
					.left_bold_text
						= t(:contractor_skills)
				.span7
					.right_text
						%ul
							- [1, 2, 3].each do |skill|
								%li Здесь будут умения #{skill}
			.row-fluid
				.span5
					.left_bold_text
						= t(:contractor_cost)
				.span7
					.right_text
						= @user.price || '0'
						= t(:rusrubles)

	- if @user.role == 'client'
		= javascript_include_tag 'Calendar/client-home-show'
		#calendar

