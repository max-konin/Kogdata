%html
  %head
    = stylesheet_link_tag 'office'
    = stylesheet_link_tag 'home'
    = javascript_include_tag "application"
    = javascript_include_tag "office"
    = javascript_include_tag "calendar"
    = javascript_include_tag "image_uploader"
  %title Kogdata
%body
  = render 'application/header'
  %p.notice
    = notice
  %p.alert
    = alert
  .container-fluid
    .row-fluid
      .sidebar#container
        =render 'office/sidebar_box'
        =render 'office/event_box'
      .span9
        = yield
