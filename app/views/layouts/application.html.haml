!!!
%html
  %head
    %meta{:charset => 'utf-8'}
    = stylesheet_link_tag 'profile'
    = stylesheet_link_tag 'home'
    = javascript_tag "var AUTH_TOKEN = #{form_authenticity_token.inspect};" if protect_against_forgery?
    = javascript_include_tag "application"
    = javascript_include_tag "common"
    = javascript_include_tag "image_uploader"
    = stylesheet_link_tag "application"
    = stylesheet_link_tag "registration"
    = javascript_include_tag "registration"
    = javascript_include_tag "social_link"
    = javascript_include_tag "validation"
    = javascript_include_tag 'modernizr.custom.17475'
    = javascript_include_tag 'Calendar/profile'

    %title Kogdata
  %body
    = render 'application/header'
    -if user_signed_in? then
      .container
        .row
          .span3
            =render 'office/sidebar_box'
          .span9
            = yield
    -else
      .container
        = yield
